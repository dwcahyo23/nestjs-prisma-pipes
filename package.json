{
	"name": "@dwcahyo/nestjs-prisma-pipes",
	"version": "2.5.5",
	"publishConfig": {
		"access": "public"
	},
	"description": "NestJS pipes collection for Prisma",
	"main": "./dist/backend/index.js",
	"types": "./dist/backend/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"exports": {
		".": {
			"types": "./dist/backend/index.d.ts",
			"import": "./dist/backend/index.js",
			"require": "./dist/backend/index.js"
		},
		"./backend": {
			"types": "./dist/backend/index.d.ts",
			"import": "./dist/backend/index.js",
			"require": "./dist/backend/index.js"
		},
		"./frontend": {
			"types": "./dist/frontend/index.d.ts",
			"import": "./dist/frontend/index.js",
			"require": "./dist/frontend/index.js"
		},
		"./package.json": "./package.json"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/backend/index.d.ts"
			],
			"backend": [
				"./dist/backend/index.d.ts"
			],
			"frontend": [
				"./dist/frontend/index.d.ts"
			]
		}
	},
	"sideEffects": false,
	"scripts": {
		"clean": "rimraf dist",
		"build": "npm run clean && npm run build:backend && npm run build:frontend",
		"build:backend": "tsc -p tsconfig.backend.json",
		"build:frontend": "tsc -p tsconfig.frontend.json",
		"prepublishOnly": "npm run build",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dwcahyo23/nestjs-prisma-pipes.git"
	},
	"keywords": [
		"nestjs",
		"pipes",
		"prisma",
		"wherepipe",
		"orderby",
		"typescript"
	],
	"author": "Yusuf Dwi",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dwcahyo23/nestjs-prisma-pipes/issues"
	},
	"homepage": "https://github.com/dwcahyo23/nestjs-prisma-pipes#readme",
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@nestjs/common": "^11.1.9",
		"@nestjs/core": "^11.1.9",
		"@nestjs/testing": "^11.0.19",
		"@types/crypto-js": "^4.2.2",
		"@types/jest": "^29.5.14",
		"@types/node": "^25.0.2",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.33.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.27.5",
		"jest": "^29.7.0",
		"reflect-metadata": "^0.2.2",
		"rimraf": "^6.1.2",
		"rxjs": "^7.8.2",
		"ts-jest": "^29.3.2",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
		"@nestjs/core": "^9.0.0 || ^10.0.0 || ^11.0.0",
		"react": "^16.8.0 || ^17.0.0 || ^18.0.0"
	},
	"peerDependenciesMeta": {
		"@nestjs/common": {
			"optional": true
		},
		"@nestjs/core": {
			"optional": true
		},
		"react": {
			"optional": true
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"dependencies": {
		"@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0",
		"crypto-js": "^4.2.0"
	}
}
