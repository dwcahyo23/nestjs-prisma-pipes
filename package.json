{
	"name": "@dwcahyo/nestjs-prisma-pipes",
	"version": "2.4.10",
	"publishConfig": {
		"access": "public"
	},
	"description": "NestJS pipes collection for Prisma (forked and improved from @nodeteam/nestjs-prisma-pipes)",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "tsc -p tsconfig.build.json",
		"prepublishOnly": "npm run build",
		"test": "jest",
		"test:watch": "jest --watch"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dwcahyo23/nestjs-prisma-pipes.git"
	},
	"keywords": [
		"nestjs",
		"pipes",
		"prisma",
		"wherepipe",
		"orderby",
		"typescript"
	],
	"author": "Yusuf Dwi",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dwcahyo23/nestjs-prisma-pipes/issues"
	},
	"homepage": "https://github.com/dwcahyo23/nestjs-prisma-pipes#readme",
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@types/jest": "^29.5.14",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.33.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.27.5",
		"ts-jest": "^29.3.2",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@nestjs/common": "^11.0.19",
		"@nestjs/testing": "^11.0.19",
		"jest": "^29.7.0"
	},
	"peerDependencies": {
		"@nestjs/common": "^9.0.0 || ^10.0.0 || ^11.0.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}